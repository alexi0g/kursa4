{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index1.html"
    },
    {
      "src": "/page2",
      "dest": "index2.html"
    },
    {
      "src": "(.*)",
      "dest": "$1"
    },
    {
      "src": "(.*).css",
      "dest": "$1.css"
    },
    {
      "src": "images/(.*)",
      "dest": "images/$1"
    }
  ]
}